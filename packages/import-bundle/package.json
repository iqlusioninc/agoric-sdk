{
  "name": "@agoric/import-bundle",
  "version": "0.1.0",
  "description": "load modules created by @agoric/bundle-source",
  "parsers": {
    "js": "mjs"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "engines": {
    "node": ">=10.15.1"
  },
  "scripts": {
    "test": "ava",
    "test:nyc": "nyc ava",
    "build": "exit 0",
    "lint-fix": "eslint --fix '**/*.js'",
    "lint-check": "eslint '**/*.js'"
  },
  "dependencies": {
    "@agoric/base64": "^0.0.0+1-dev",
    "@agoric/assert": "^0.1.0",
    "@agoric/compartment-mapper": "^0.2.3"
  },
  "devDependencies": {
    "@agoric/bundle-source": "^1.1.10",
    "@agoric/install-ses": "^0.4.0",
    "ava": "^3.12.1",
    "esm": "^3.2.5",
    "nyc": "^15.1.0"
  },
  "peerDependencies": {
    "ses": "^0.11.0"
  },
  "files": [
    "README.md",
    "LICENSE",
    "NEWS.md",
    "src/**/*.js"
  ],
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric-sdk/issues"
  },
  "homepage": "https://github.com/Agoric/agoric-sdk/packages/import-bundle",
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "esm"
    ],
    "timeout": "2m"
  }
}
